{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "tsc && node ./build/src/server.js",
    "start:dev": "nodemon --config nodemon.json src/server.ts",
    "lint": "eslint {src,test}/**/*",
    "lint:fix": "eslint {src,test}/**/* --fix",
    "prettier:write": "npx prettier --write {src,test}/**/*",
    "prettier:check": "npx prettier --check {src,test}/**/*",
    "test": "jest",
    "db:migrations:make": "knex --knexfile src/db/knexfile.ts migrate:make",
    "db:migrations:latest": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile src/db/knexfile.ts migrate:latest",
    "db:migrations:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile src/db/knexfile.ts migrate:rollback"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/knex": "^0.16.1",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "prettier": "2.3.0",
    "ts-jest": "^27.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "mysql2": "^2.2.5"
  }
}
